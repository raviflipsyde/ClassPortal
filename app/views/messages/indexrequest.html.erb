<p id="notice"><%= notice %></p>

<h1>Listing Requests</h1>

<table>
  <thead>
    <tr>
      <th>Msg</th>
      <th>From user</th>
      <th>Readflag</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @messages.each do |message| %>
      <tr>
        <td><%= message.msg %></td>
        <td><%= message.from_user %></td>
        <td><%= message.readflag ? 'Read': 'Unread' %></td>
        <td><%= link_to 'Show', message %></td>
        <!--<td><%= link_to 'Edit', edit_message_path(message) %></td>-->
        <td><%= link_to 'Delete', message, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Back', current_user  %>

